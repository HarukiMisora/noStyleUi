{
  "name": "nostyleui",
  "private": false,
  "version": "0.4.0-beta.13",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc vite build",
    "preview": "vite preview",
    "build:pack":"npm run build:test && npm publish",
    "build:test": "npm run clear && npm run build:types && vite build --emptyOutDir false && npm run build:sass",
    "build:sass": "sass src/noStyleUi/div/style/css.scss dist/nostyleui.css -s compressed --no-source-map",
    "build:types": "tsc -p tsconfig.lib.json",
    "clear": "node scripts/clear.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vue/compiler-dom": "^3.5.13",
    "sass": "^1.85.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vicons/ionicons5": "^0.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vue-tsc": "^2.2.0"
  },
  "description": "-",
  "main": "./dist/my-ui-library.umd.js",
  "module": "./dist/my-ui-library.es.js",
  "types": "./dist/types/interface.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/interface.d.ts",
      "import": "./dist/no-style-ui.es.js",
      "require": "./dist/no-style-ui.umd.js"
    },
    "./index": {
      "types": "./dist/types/interface.d.ts",
      "import": "./dist/no-style-ui.es.js",
      "require": "./dist/no-style-ui.umd.js"
    },
    "./*": "./dist/*",
    "./style": "./dist/nostyleui.css"
  },
  "keywords": [
    "nostyleui"
  ],
  "author": "wzc",
  "license": "ISC"
}
